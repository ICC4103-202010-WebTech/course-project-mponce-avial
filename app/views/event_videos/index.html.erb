<p id="notice"><%= notice %></p>

<h1>Event Videos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_videos.each do |event_video| %>
      <tr>
        <td><%= link_to 'Show', event_video %></td>
        <td><%= link_to 'Edit', edit_event_video_path(event_video) %></td>
        <td><%= link_to 'Destroy', event_video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Video', new_event_video_path %>
