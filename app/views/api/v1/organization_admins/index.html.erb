<p id="notice"><%= notice %></p>

<h1>Organization Admins</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_admins.each do |organization_admin| %>
      <tr>
        <td><%= link_to 'Show', organization_admin %></td>
        <td><%= link_to 'Edit', edit_organization_admin_path(organization_admin) %></td>
        <td><%= link_to 'Destroy', organization_admin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Organization Admin', new_organization_admin_path %>
