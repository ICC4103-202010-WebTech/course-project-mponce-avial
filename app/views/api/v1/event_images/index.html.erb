<p id="notice"><%= notice %></p>

<h1>Event Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_images.each do |event_image| %>
      <tr>
        <td><%= link_to 'Show', event_image %></td>
        <td><%= link_to 'Edit', edit_event_image_path(event_image) %></td>
        <td><%= link_to 'Destroy', event_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Image', new_event_image_path %>
