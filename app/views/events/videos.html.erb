<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <h1>Videos</h1>
  <hr>
</head>

<% if @event.videosEvent.attached? %>
  <%@event.videosEvent.each do |videos| %>
    <div>
      <%= video_tag(rails_blob_path(videos),:size => "320x240", :controls => true) %>
      <h4><%=videos.filename%></h4>
    </div>
  <% end %>
<% else %>
<% end %>